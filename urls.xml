<?xml version="1.0" encoding="utf-8"?>
<tryton>
    <data >
        <record id="sale" model="nereid.url_rule">
            <field name="rule">/sale/</field>
            <field name="endpoint">sale.sale.render_list</field>
            <field name="sequence" eval="10" />
            <field name="url_map" ref="nereid.default_url_map" />
        </record>
        <record id="sale_detail" model="nereid.url_rule">
            <field name="rule">/sale/&lt;uri&gt;</field>
            <field name="endpoint">sale.sale.render</field>
            <field name="sequence" eval="20" />
            <field name="url_map" ref="nereid.default_url_map" />
        </record>
    </data>
</tryton>
